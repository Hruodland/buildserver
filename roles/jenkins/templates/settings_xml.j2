<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
    <mirrors>
        <mirror>
            <id>nexus-snapshot</id>
            <mirrorOf>snapshots</mirrorOf>
            <url>https://localhost/nexus/content/groups/public-snapshots</url>
        </mirror>
        <mirror>
            <id>nexus</id>
            <mirrorOf>*,!snapshots</mirrorOf>
            <url>https://localhost/nexus/content/groups/public</url>
        </mirror>
    </mirrors>

    <profiles>
        <profile>
            <id>nexus</id>
            <pluginRepositories>
                <pluginRepository>
                    <id>snapshots</id>
                    <url>https://localhost/nexus</url>
                    <releases><enabled>false</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>central</id>
                    <url>https://localhost/nexus</url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>false</enabled></snapshots>
                </pluginRepository>
            </pluginRepositories>
            <repositories>
                <repository>
                    <id>snapshots</id>
                    <url>https://localhost/nexus</url>
                    <releases><enabled>false</enabled></releases>
                    <snapshots><enabled>true</enabled></snapshots>
                </repository>
                <repository>
                    <id>central</id>
                    <url>https://localhost/nexus/url>
                    <releases><enabled>true</enabled></releases>
                    <snapshots><enabled>false</enabled></snapshots>
                </repository>
            </repositories>
        </profile>
         <profile>
		  <repositories>
			<repository>
			  <snapshots>
				<enabled>false</enabled>
			  </snapshots>
			  <id>central</id>
			  <name>libs-release</name>
			  <url>https://localhost/artifactory/libs-release</url>
			</repository>
			<repository>
			  <snapshots />
			  <id>snapshots</id>
			  <name>libs-snapshot</name>
			  <url>https://localhost/artifactory/libs-snapshot</url>
			</repository>
		  </repositories>
		  <pluginRepositories>
			<pluginRepository>
			  <snapshots>
				<enabled>false</enabled>
			  </snapshots>
			  <id>central</id>
			  <name>plugins-release</name>
			  <url>https://localhost/artifactory/plugins-release</url>
			</pluginRepository>
			<pluginRepository>
			  <snapshots />
			  <id>snapshots</id>
			  <name>plugins-snapshot</name>
			  <url>https://localhost/artifactory/plugins-snapshot</url>
			</pluginRepository>
		  </pluginRepositories>
		  <id>artifactory</id>
		</profile>
    </profiles>
    <servers>
        <server>
            <id>nexus</id>
            <username>admin</username>
            <password>admin123</password>
        </server>
        <server>
            <id>artifactory</id>
            <username>admin</username>
            <password>AP41frEADixDgC1DrR18nos47S3</password>
        </server>
    </servers>


</settings>
